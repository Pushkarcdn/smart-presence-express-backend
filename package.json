    "express": "^4.17.1",
    "express-http-context": "^1.2.4",
    "express-oas-generator": "^1.0.46",
    "express-session": "^1.17.2",
    "glob": "^7.1.7",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "joi": "^17.9.2",
    "joi-to-swagger": "^6.2.0",
    "jsonwebtoken": "^9.0.2",
    "keycloak-connect": "^26.0.8",
    "mongoose": "^6.0.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.12.0",
