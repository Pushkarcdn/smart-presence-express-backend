{
  "name": "smart-presence-express-backend",
  "version": "0.0.3",
  "description": "This is a mvp version of Smart Presence Project built using JS and Express",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "dev": "cross-env NODE_ENV=development nodemon --inspect",
    "migration:generate": "npx sequelize-cli migration:generate --name",
    "migration:run": "npx sequelize-cli db:migrate",
    "migration:revert": "npx sequelize-cli db:migrate:undo",
    "seed:generate": "npx sequelize-cli seed:generate",
    "seed:run": "npx sequelize-cli db:seed",
    "seed:revert": "npx sequelize-cli db:seed:undo",
    "lint": "eslint --fix . && echo 'Lint complete.'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pushkarcdn/smart-presence-express-backend.git"
  },
  "keywords": [
    "JS",
    "Node.js",
    "Express",
    "Postgres",
    "RabbitMQ",
    "Redis",
    "Socket.IO"
  ],
  "author": "pushkarcdn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pushkarcdn/smart-presence-express-backend/issues"
  },
  "homepage": "https://github.com/pushkarcdn/smart-presence-express-backend#readme",
